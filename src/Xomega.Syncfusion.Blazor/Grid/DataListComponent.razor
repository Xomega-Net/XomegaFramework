@namespace Xomega._Syncfusion.Blazor
@using Xomega.Framework;

@inherits DataListAdaptor

<CascadingValue Value="@this">
    @ChildContent
</CascadingValue>

@code {
    [Parameter]
    public RenderFragment ChildContent { get; set; }

    [Parameter]
    public DataListObject List{ get; set; }

    protected override DataListObject GetListObject() => List ?? base.GetListObject();
}
