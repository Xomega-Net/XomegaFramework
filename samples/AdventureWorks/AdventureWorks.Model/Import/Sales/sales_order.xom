            </operation>
            <operation name="update" type="update">
              <input>
                <param name="sales order id" type="sales order" required="true"/>
                <param name="sales reason id"/>
                <struct name="data">
                  <param name="modified date"/>
                  <config>
                    <xfk:add-to-object class="SalesOrderReasonObject"/>
                  </config>
                </struct>
                <config>
                  <xfk:add-to-object class="SalesOrderReasonObject"/>
                </config>
              </input>
              <doc>
                <summary>Updates existing or creates a new Sales Order Reason object using the specified data.</summary>
