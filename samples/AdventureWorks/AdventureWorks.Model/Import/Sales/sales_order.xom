            </operation>
            <operation name="delete" type="delete">
              <input>
                <param name="sales order id" type="sales order" required="true"/>
                <param name="sales reason id"/>
                <config>
                  <xfk:add-to-object class="SalesOrderReasonObject"/>
                </config>
              </input>
              <doc>
                <summary>Deletes the specified Sales Order Reason object.</summary>
              </doc>
            </operation>
            <operation name="read list" type="readlist">
              <input>
                <param name="sales order id" type="sales order" required="true"/>
              </input>
              <output list="true">
                <param name="sales reason id"/>
                <param name="modified date"/>
                <config>
                  <xfk:add-to-object class="SalesOrderReasonList"/>
                </config>
              </output>
              <config/>
              <doc>
                <summary>Reads a list of Sales Order Reason objects based on the specified criteria.</summary>
              </doc>
