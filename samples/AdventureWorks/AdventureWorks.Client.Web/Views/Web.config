<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.web>
    <authorization>
      <deny users="?"/>
    </authorization>
  </system.web>
  <location path="Person/LoginViewPage.aspx">
    <system.web>
      <authorization>
        <allow users="*"/>
      </authorization>
    </system.web>
  </location>
  <location path="Sales">
    <system.web>
      <authorization>
        <allow roles="SP, SC, IN, EM"/>
        <deny users="*"/>
      </authorization>
    </system.web>
  </location>
</configuration>